            </li>
            <li>
                <a href="https://pro.m.jd.com/mall/active/2hqsQcyM5bEUVSStkN3BwrBHqVLd/index.html">
                    <img src="./imgs/nav7.png" alt=""/>
                    <p>京东超市</p>
                </a>
            </li>
        </ul>
    </nav>
    <!-- 中间banner-->
    <div class="index_banner">
        <img src="./imgs/新人188.jpg" alt="">
    </div>
    <!--商品-->
    <main class="jd_product">
        <!--商品子盒子-->
        <section class="product_box jd_sk">
            <!--头部-->
            <div class="product_box_tit no_border">
                <div class="f_left m_l10">
                    <span class="sk_icon"></span>
                    <span class="sk_name m_l10">掌上秒杀</span>
                    <div class="sk_time m_l10">
                        <span>0</span>
                        <span>0</span>
                        <span>:</span>
                        <span>0</span>
                        <span>0</span>
                        <span>:</span>
                        <span>0</span>
                        <span>0</span>
                    </div>
                </div>
                <div class="f_right m_r10"><a href="#">更多></a></div>
            </div>
            <!--内容-->
            <div class="product_box_con">
                <ul class="clearfix">
                    <li>
                        <a href="#"><img src="./imgs/detail01.jpg" alt=""/></a>
                        <p>&yen;10.00</p>
                        <p>&yen;100.00</p>
                    </li>
                    <li>
                        <a href="#"><img src="./imgs/detail02.jpg" alt=""/></a>
                        <p>&yen;10.00</p>
