            <li></li>
            <li></li>
            <li></li>
            <li></li>
        </ul>
    </div>
    <!--导航栏-->
    <nav class="jd_nav">
        <ul class="clearfix">
            <li>
                <a href="https://so.m.jd.com/category/all.html">
                    <img src="./imgs/nav0.png" alt=""/>
                    <p>分类查询</p>
                </a>
            </li>
            <li>
                <a href="https://m.jd.com/">
                    <img src="./imgs/nav1.png" alt=""/>
