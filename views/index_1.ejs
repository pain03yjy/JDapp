                    </li>
                </ul>
            </div>
        </section>
        <!-- 中间banner-->
        <div class="index_banner">
            <img src="./imgs/超级单品日.jpg" alt="">
        </div>
        <!--商品子盒子-->
        <section class="product_box">
            <!--头部-->
            <div class="product_box_tit"><h3>京东商城</h3></div>
            <!--内容-->
            <div class="product_box_con clearfix">
                <a href="#" class="f_left w_50 b_right"><img src="./imgs/cp1.jpg" alt=""/></a>
                <a href="#" class="f_right w_50 b_bottom"><img src="./imgs/cp2.jpg" alt=""/></a>
                <a href="#" class="f_right w_50 "><img src="./imgs/cp3.jpg" alt=""/></a>
            </div>
        </section>
        <!--商品子盒子-->
        <section class="product_box">
            <!--头部-->
            <div class="product_box_tit"><h3>京东超市</h3></div>
            <!--内容-->
            <div class="product_box_con clearfix">
                <a href="#" class="f_right w_50 b_left"><img src="./imgs/cp4.jpg" alt=""/></a>
                <a href="#" class="f_left w_50 b_bottom"><img src="./imgs/cp5.jpg" alt=""/></a>
                <a href="#" class="f_left w_50"><img src="./imgs/cp6.jpg" alt=""/></a>
            </div>
        </section>
        <!--商品子盒子-->
        <section class="product_box">
            <!--头部-->
            <div class="product_box_tit"><h3>发现好货</h3></div>
            <!--内容-->
            <!--内容-->
            <div class="product_box_con clearfix">
                <ul class="jd_list" >
                    <%for(var i=0;i<shops.length;i++){%>
                    <li>
                        <img src="./imgs/<%=shops[i].shop_name%>.jpg" alt="" width="205" height="197">
                        <p><%=shops[i].shop_detail%></p>
                        <p>&yen;<%=shops[i].price%>  <span class="glyphicon glyphicon-shopping-cart"></span></p>
                    </li>
                    <%}%>
                    <br>
                    <br>
                    <br>
                    <br>
                    <br>
                    <br>
                    <br>
                </ul>
            </div>
        </section>
    </main>
    <footer class="jd_footer">
        <div class="jd_footer_box">
            <div>
                <a href="/">
                        <span class="glyphicon glyphicon-home jdcolor">
                       <p>首页</p>
                    </span>
                </a>
            </div>
            <div>
                <a href="/shopping">
                        <span class="glyphicon glyphicon-shopping-cart">
                             <p>购物车</p>
                        </span>
                </a>
            </div>
            <div>
                <a href="/my">
                        <span class="glyphicon glyphicon-user">
                            <p>我的</p>
                        </span>
                </a>
            </div>

        </div>
